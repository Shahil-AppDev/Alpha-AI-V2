{
  "llm": {
    "endpoint": "http://llm-service:8000/generate",
    "api_key": "test-key",
    "model": "gpt-3.5-turbo",
    "max_tokens": 2000,
    "temperature": 0.7,
    "timeout": 30
  },
  "agent": {
    "require_human_approval": true,
    "max_tool_calls": 5,
    "memory_size": 100
  },
  "security": {
    "enable_logging": true,
    "log_level": "INFO",
    "audit_actions": true
  },
  "modules": {
    "osint": {
      "enabled": true,
      "timeout": 30
    },
    "network": {
      "enabled": true,
      "timeout": 60
    },
    "password": {
      "enabled": true,
      "wordlist_path": "/tools/wordlists/"
    },
    "analysis": {
      "enabled": true,
      "timeout": 45
    },
    "exploit": {
      "enabled": true,
      "require_approval": true
    },
    "reverse_engineering": {
      "enabled": true,
      "require_approval": true
    }
  }
}
